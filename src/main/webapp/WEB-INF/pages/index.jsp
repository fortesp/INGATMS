<%@ page contentType="text/html; charset=UTF-8" %><!DOCTYPE html><html><head>   <title>ING - ATMS</title>    <script type="text/javascript" src="/js/angular.min.js"></script>    <script>        var app = angular.module('myApp', []);        app.controller('myCtrl', function($scope, $http) {           $scope.callService = function(cityVal) {               $http({                   url: "atms",                   method: "GET",                   params: { city: cityVal }               }).then(function mySuccess(response) {                   $scope.atms = response.data;               }, function myError(response) {                   $scope.atms = response.statusText;               });           }            $scope.city = "Amsterdam";            $scope.callService($scope.city);        });   </script>    <style type="text/css">        * {            font: 12px arial, sans-serif;        }        table {            width: 800px;        }        th {            padding: 7px;            margin: 3px;            background: orange;            color: #FFF;        }        td {            padding: 7px;            margin: 3px;            background: #F1F1F1;        }        #holder {            width: 800px;            margin: 0 auto;        }    </style></head><body><div id="holder" ng-app="myApp" ng-controller="myCtrl">    <form name="frm1" ng-submit="callService(city)">        <input id="city"  ng-model="city" type="text" value="Search for a city..." style="width:90%; padding: 2px; margin:2px; color: lightgrey"               onFocus="this.value=''; this.style.color='black'"               onBlur="this.value='Search for a city...'; this.style.color='lightgrey'"/>        <input type="submit" value="Filter" style="border:1px solid grey; background: white;" />    </form>        <table>        <thead>            <tr>                <th>Street</th>                <th>House Number</th>                <th>Postal Code</th>                <th>City</th>                <th>Latitude</th>                <th>Longitude</th>                <th>Distance</th>                <th>Type</th>            </tr>        </thead>        <tr ng-repeat="atm in atms">            <td>{{ atm.address.street }}</td>            <td>{{ atm.address.housenumber }}</td>            <td>{{ atm.address.postalcode }}</td>            <td>{{ atm.address.city }}</td>            <td>{{ atm.address.geoLocation.lat }}</td>            <td>{{ atm.address.geoLocation.lng }}</td>            <td>{{ atm.distance }}</td>            <td>{{ atm.type }}</td>        </tr>        <tr ng-hide="atms.length"><td align="center" colspan="8" style="background:white;">No ATMS available in that location or unknown location.</td></tr>    </table></div></body></html>